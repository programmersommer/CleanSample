{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "@microsoft/signalr@6.0.8",
      "destination": "wwwroot/signalr/"
    },
    {
      "library": "bootstrap@5.2.0",
      "destination": "wwwroot/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.6.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "library": "jquery-validation@1.19.5",
      "destination": "wwwroot/jquery-validation/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/jquery-validation-unobtrusive/"
    }
  ]
}